{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build && mkdir -p public/build/shiki/dist && cp -r node_modules/shiki/languages node_modules/shiki/themes public/build/shiki && cp node_modules/shiki/dist/onig.wasm public/build/shiki/dist/onig.wasm",
