    "cross-env": "^7.0.3",
    "isbot": "^3.6.5",
    "kysely": "0.22.0",
    "kysely-d1": "0.0.5",
    "marked": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
